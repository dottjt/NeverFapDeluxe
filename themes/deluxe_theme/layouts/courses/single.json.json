{{ define "response" }} 
{
  "title": {{ .Title | jsonify }},
  "description": {{ .Description | jsonify }},
  "content": {{ .Content | jsonify }},
  "date": {{ .Date.Format "2006-01-02" | jsonify }},
  "draft": {{ .Draft | jsonify }},
  "slug": {{ .Params.Slug | jsonify }},
  "categories": {{ .Params.Categories | jsonify }}
  "category": {{ .Params.Category | jsonify }}
  "tags": {{ .Params.Tags | jsonify }},
  "permalink" : {{ .Permalink | jsonify }},

  "overview": {{ if .Params.Overview }}{{ .Params.Overview | jsonify }}{{ end }},
  "learn1": {{ if .Params.Learn1 }}{{ .Params.Learn1 | jsonify }}{{ end }},
  "learn2": {{ if .Params.Learn2 }}{{ .Params.Learn2 | jsonify }}{{ end }},
  "learn3": {{ if .Params.Learn3 }}{{ .Params.Learn3 | jsonify }}{{ end }}
}
{{ end }}

